<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Facebook Automation Script " parallel="true">

    <parameter name="useCloudEnv" value="true"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="60.0"/>
    <parameter name="url" value="https://www.facebook.com/"/>


    <test name="Facebook Test" enabled="true">
        <classes>
            <class name="pageobjecttest.LoginPageTest"/>
            <class name="pageobjecttest.SearchTest"/>
            <class name="googlesheettestPack.GoogleSheetTest"/>

        </classes>
    </test>
</suite>